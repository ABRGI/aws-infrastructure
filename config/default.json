{
    "environmentname": "Nelson",
    "domain": "admin.nelson.management",
    "hostedzonestack": {
        "hostedzone": "nelson.management",
        "createnewsubdomaincertificate": true,
        "createnelsonmanagementcloudfrontentry": true
    },
    "adminloginpath": "#/login",
    "landingpage": "#/users",
    "applicationname": "Nelson Management",
    "nelsonloginproviderstack": {
        "nelsonuserpool": "test-nelson-users",
        "guestuserpool": "test-guest-users",
        "dynamogettables": [
            "user-management-access-roles",
            "user-management-access-rights",
            "user-management-users"
        ]
    },
    "nelsonusermanagementservicetack": {
        "usertable": "user-management-users",
        "accessrolestable": "user-management-access-roles",
        "accessrightstable": "user-management-access-rights"
    },
    "useexistingvpc": false,
    "existingvpcname": "",
    "vpcservice": {
        "cidr": "17.0.0.0/16",
        "maxnatgateways": 1,
        "maxazs": 2,
        "cidrMask": 20
    },
    "defaultremovalpolicy": "snapshot",
    "nelsonsaasbucket": {
        "buibucketname": "test.bookn.io",
        "useexistingbuibucket": false,
        "muibucketname": "test.nelson.management",
        "useexistingmuibucket": false,
        "publicreadaccess": true
    }
}