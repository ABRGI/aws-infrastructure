{
  "name": "aws-infrastructure",
  "version": "0.1.0",
  "bin": {
    "aws-infrastructure": "bin/aws-infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "startapp": "npx ts-node --prefer-ts-exts bin/$appscript.ts",
    "diff": "cdk diff $stack --app  \"npm run startapp\"",
    "synth": "cdk synth $stack --app  \"npm run startapp\"",
    "deploy": "cdk deploy $stack --app  \"npm run startapp\"",
    "destroy": "cdk destroy $stack --app  \"npm run startapp\""
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.15",
    "@types/source-map-support": "^0.5.6",
    "aws-cdk": "2.56.0",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.56.0",
    "config": "^3.3.8",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
